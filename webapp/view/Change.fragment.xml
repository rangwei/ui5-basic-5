<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form"
	xmlns:core="sap.ui.core">
	<VBox class="sapUiSmallMargin">
		<f:Form id="FormChange354"
			editable="true">
			<f:title>
				<core:Title text="Product" />
			</f:title>
			<f:layout>
				<f:ResponsiveGridLayout
					labelSpanXL="3"
					labelSpanL="3"
					labelSpanM="3"
					labelSpanS="12"
					adjustLabelSpan="false"
					emptySpanXL="4"
					emptySpanL="4"
					emptySpanM="4"
					emptySpanS="0"
					columnsXL="1"
					columnsL="1"
					columnsM="1"
					singleContainerFullSize="false" />
			</f:layout>
			<f:formContainers>
				<f:FormContainer>
					<f:formElements>
						<f:FormElement label="Product Name">
							<f:fields>
								<Input value="{invoice>/Invoices/0/ProductName}" id="name" change="onChange"/>
							</f:fields>
						</f:FormElement>
						<f:FormElement label="Quantity">
							<f:fields>
								<Input value="{invoice>/Invoices/0/Quantity}" />
								
							</f:fields>
						</f:FormElement>
						<f:FormElement label="Price">
							<f:fields>
								
								<Input value="{invoice>/Invoices/0/ExtendedPrice}" />
							</f:fields>
						</f:FormElement>
						<f:FormElement label="Shipper Name">
							<f:fields>
								<Input value="{invoice>/Invoices/0/ShipperName}" />
							</f:fields>
						</f:FormElement>
					</f:formElements>
				</f:FormContainer>
			</f:formContainers>
		</f:Form>
	</VBox>
</core:FragmentDefinition>